{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.ts",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html",
        "headers": {
          "Content-Type": "text/javascript"
        }
      }
    ]
  }